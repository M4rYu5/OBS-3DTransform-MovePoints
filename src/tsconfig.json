{
    "compilerOptions": {
      "module": "system",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "outFile": "./wwwroot/js/ts-generated.js",
      "sourceMap": true,
      "target": "ES2022"
    },
    "include": ["ts/**/*"],
    "exclude": ["node_modules", "**/*.spec.ts"]
}